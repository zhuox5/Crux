
test(a, b) {
  printInt((a + b));
  println();
  printInt((a - b));
  println();
  printInt((a * b));
  println();
  if ((b != 0)) {
    printInt((a / b));
    println();
  } else {
    println();
  }
}
main() {
  i;
  j;
  for(i = 0;(i <= 10);i = (i + 1)) {
    for(j = 0;(j <= 10);j = (j + 1)) {
      test(i, j);
    }
  }
}

